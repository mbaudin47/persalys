#                                               -*- cmake -*-
#
#  OTGuiConfig.cmake(.in)
#
#  Find OTGui includes and libraries
#
#  Copyright (C) 2005-2014 Phimeca
#

# Use the following variables to compile and link against OTGui:
#  OTGUI_FOUND          - True if OTGui was found on your system
#  OTGUI_USE_FILE       - The file making OTGui usable
#  OTGUI_DEFINITIONS    - Definitions needed to build with OTGui
#  OTGUI_INCLUDE_DIRS   - List of directories where OTGui' header file are
#  OTGUI_LIBRARY        - Library name
#  OTGUI_LIBRARIES      - List of libraries to link against
#  OTGUI_LIBRARY_DIRS   - List of directories containing OTGui' libraries
#  OTGUI_ROOT_DIR       - The base directory of OTGui
#  OTGUI_VERSION_STRING - A human-readable string containing the version
#  OTGUI_VERSION_MAJOR  - The major version of OTGui
#  OTGUI_VERSION_MINOR  - The minor version of OTGui
#  OTGUI_VERSION_PATCH  - The patch version of OTGui

set ( OTGUI_FOUND 1 )
set ( OTGUI_USE_FILE     "@OTGUI_USE_FILE@" )

set ( OTGUI_DEFINITIONS  "@OTGUI_DEFINITIONS@" )
set ( OTGUI_INCLUDE_DIR  "@OTGUI_INCLUDE_DIR@" )
set ( OTGUI_INCLUDE_DIRS "@OTGUI_INCLUDE_DIRS@" )
set ( OTGUI_LIBRARY      "@OTGUI_LIBRARY@" )
set ( OTGUI_LIBRARIES    "@OTGUI_LIBRARIES@" )
set ( OTGUI_LIBRARY_DIRS "@OTGUI_LIBRARY_DIRS@" )
set ( OTGUI_ROOT_DIR     "@OTGUI_ROOT_DIR@" )

set ( OTGUI_VERSION_STRING "@OTGUI_VERSION_STRING@" )
set ( OTGUI_VERSION_MAJOR  "@OTGUI_VERSION_MAJOR@" )
set ( OTGUI_VERSION_MINOR  "@OTGUI_VERSION_MINOR@" )
set ( OTGUI_VERSION_PATCH  "@OTGUI_VERSION_PATCH@" )

set (OTGUI_PYTHON@PYTHON_VERSION_MAJOR@_MODULE_PATH "@OTGUI_PYTHON_MODULE_PATH@")
