#!/usr/bin/env python
# coding: utf-8

from __future__ import print_function
import openturns as ot
import otguibase

study1 = otguibase.OTStudy('study1')
otguibase.OTStudy.Add(study1)
dist_Q = ot.Normal(10200, 100)
Q = otguibase.Input('Q', 0, "Primary energy", dist_Q)
dist_E = ot.Normal(3000, 15)
E = otguibase.Input('E', 0, "Produced electric energy", dist_E)
dist_C = ot.Normal(4000, 60)
C = otguibase.Input('C', 0, "Valued thermal energy", dist_C)
Ep = otguibase.Output('Ep', 0, "Primary energy savings", '1-(Q/((E/((1-0.05)*0.54))+(C/0.8)))')
analyticalModel1 = otguibase.AnalyticalPhysicalModel('analyticalModel1')
analyticalModel1.addInput(Q)
analyticalModel1.addInput(E)
analyticalModel1.addInput(C)
analyticalModel1.addOutput(Ep)
study1.add(analyticalModel1)
dist_Q = ot.Normal(10200, 100)
Q = otguibase.Input('Q', 0, "Primary energy", dist_Q)
dist_E = ot.Normal(3000, 15)
E = otguibase.Input('E', 0, "Produced electric energy", dist_E)
dist_C = ot.Normal(4000, 60)
C = otguibase.Input('C', 0, "Valued thermal energy", dist_C)
Ep = otguibase.Output('Ep', 0, "Primary energy savings", '1-(Q/((E/((1-0.05)*0.54))+(C/0.8)))')
pythonModel1 = otguibase.PythonPhysicalModel('pythonModel1')
pythonModel1.addInput(Q)
pythonModel1.addInput(E)
pythonModel1.addInput(C)
pythonModel1.addOutput(Ep)
pythonModel1.setCode('def _exec(Q, E, C):\n    Ep = 1-(Q/((E/((1-0.05)*0.54))+(C/0.8)))\n    return [Ep]\n')
study1.add(pythonModel1)

