#!/usr/bin/env python
# coding: utf-8

from __future__ import print_function
import openturns as ot
import otguibase

anOTStudy = otguibase.OTStudy('anOTStudy')
otguibase.OTStudy.Add(anOTStudy)
dist_X0 = ot.Normal(0, 1)
X0 = otguibase.Input('X0', 1, dist_X0, '')
X1 = otguibase.Input('X1', 2, '')
Y0 = otguibase.Output('Y0', '')
inputs = [X0, X1]
outputs = [Y0]
formulas = ['sin(X0)+8*X1']
aModelPhys = otguibase.SymbolicPhysicalModel('aModelPhys', inputs, outputs, formulas)
anOTStudy.add(aModelPhys)
values = [1, 2]
lowerBounds = [0.9, 1.8]
upperBounds = [1.1, 2.2]
levels = [2, 2]
aDesign_1 = otguibase.FixedDesignOfExperiment('aDesign_1', aModelPhys, lowerBounds, upperBounds, levels, values)
anOTStudy.add(aDesign_1)
aDesign_1.run()
inputColumns = [0, 2]
aDesign_2 = otguibase.FromFileDesignOfExperiment('aDesign_2', aModelPhys, 'normal.csv', inputColumns)
anOTStudy.add(aDesign_2)
aDesign_2.run()
aDesign_3 = otguibase.ProbabilisticDesignOfExperiment('aDesign_3', aModelPhys, 10, 'QUASI_MONTE_CARLO')
aDesign_3.setSeed(0)
anOTStudy.add(aDesign_3)
aDesign_3.run()

