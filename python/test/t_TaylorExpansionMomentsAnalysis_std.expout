#!/usr/bin/env python
# coding: utf-8

from __future__ import print_function
import openturns as ot
import otguibase

myStudy = otguibase.OTStudy('myStudy')
otguibase.OTStudy.Add(myStudy)
dist_X0 = ot.Normal(1, 1)
X0 = otguibase.Input('X0', 1, '', dist_X0)
dist_X1 = ot.Normal(1, 1)
X1 = otguibase.Input('X1', 2, '', dist_X1)
X2 = otguibase.Input('X2', 10, '')
fake_Y0 = otguibase.Output('fake_Y0', 0, '', 'X0')
fake_Y0.setIsSelected(False)
Y0 = otguibase.Output('Y0', 0, '', 'sin(X0) + 8*X1')
Y1 = otguibase.Output('Y1', 0, '', 'sin(X0) + 8*X1 + X2')
aModelPhys = otguibase.AnalyticalPhysicalModel('aModelPhys')
aModelPhys.addInput(X0)
aModelPhys.addInput(X1)
aModelPhys.addInput(X2)
aModelPhys.addOutput(fake_Y0)
aModelPhys.addOutput(Y0)
aModelPhys.addOutput(Y1)
myStudy.add(aModelPhys)
aTaylor = otguibase.TaylorExpansionMomentsAnalysis('aTaylor', aModelPhys)
interestVariables = ['Y0']
aTaylor.setInterestVariables(interestVariables)
myStudy.add(aTaylor)
aTaylor2 = otguibase.TaylorExpansionMomentsAnalysis('aTaylor2', aModelPhys)
interestVariables = ['Y1']
aTaylor2.setInterestVariables(interestVariables)
myStudy.add(aTaylor2)

