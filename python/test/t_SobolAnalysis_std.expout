#!/usr/bin/env python
# coding: utf-8

from __future__ import print_function
import openturns as ot
import otguibase

myStudy = otguibase.OTStudy('myStudy')
otguibase.OTStudy.Add(myStudy)
dist_x0 = ot.Normal(0, 1)
x0 = otguibase.Input('x0', 0, dist_x0, '')
dist_x1 = ot.Normal(0, 1)
x1 = otguibase.Input('x1', 0, dist_x1, '')
x2 = otguibase.Input('x2', 10, '')
fake_y0 = otguibase.Output('fake_y0', '')
fake_y0.setIsSelected(False)
y0 = otguibase.Output('y0', '')
y1 = otguibase.Output('y1', '')
inputs = [x0, x1, x2]
outputs = [fake_y0, y0, y1]
formulas = ['x0', '10+3*x0+x1', '3*x0 + x1 + x2']
aModel = otguibase.SymbolicPhysicalModel('aModel', inputs, outputs, formulas)
myStudy.add(aModel)
aSobol = otguibase.SobolAnalysis('aSobol', aModel)
interestVariables = ['y0']
aSobol.setInterestVariables(interestVariables)
aSobol.setMaximumCalls(1000)
aSobol.setMaximumCoefficientOfVariation(-1)
aSobol.setMaximumElapsedTime(30)
aSobol.setBlockSize(250)
aSobol.setSeed(2)
aSobol.setBootstrapSize(100)
aSobol.setBootstrapConfidenceLevel(0.95)
myStudy.add(aSobol)
aSobol2 = otguibase.SobolAnalysis('aSobol2', aModel)
interestVariables = ['y0']
aSobol2.setInterestVariables(interestVariables)
aSobol2.setMaximumCoefficientOfVariation(0.02)
aSobol2.setMaximumElapsedTime(100000)
aSobol2.setBlockSize(1)
aSobol2.setSeed(0)
aSobol2.setBootstrapSize(100)
aSobol2.setBootstrapConfidenceLevel(0.95)
myStudy.add(aSobol2)
aSobol3 = otguibase.SobolAnalysis('aSobol3', aModel)
aSobol3.setMaximumCalls(1000)
aSobol3.setMaximumCoefficientOfVariation(0.01)
aSobol3.setMaximumElapsedTime(60)
aSobol3.setBlockSize(250)
aSobol3.setSeed(2)
aSobol3.setBootstrapSize(100)
aSobol3.setBootstrapConfidenceLevel(0.95)
myStudy.add(aSobol3)

